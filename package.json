{
  "name": "nodejs.knexjs.typescript",
  "version": "0.0.1",
  "description": "Nodejs template",
  "dependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/es6-promise": "3.3.0",
    "aws-sdk": "2.954.0",
    "axios": "0.21.1",
    "basic-auth": "2.0.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-var": "1.1.0",
    "crypto": "1.0.1",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "faker": "4.1.0",
    "fs": "0.0.1-security",
    "hbs": "4.0.4",
    "http": "0.0.0",
    "i18n": "0.13.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.7",
    "lodash": "4.17.11",
    "log4js": "4.1.0",
    "logrotator": "1.1.0",
    "moment": "2.24.0",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "nodemailer": "^6.1.0",
    "nodemailer-express-handlebars": "3.0.0",
    "objection": "2.2.15",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pg": "8.6.0",
    "q": "1.5.1",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "socket.io": "4.4.1",
    "source-map-support": "0.5.20",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.1.6",
    "underscore": "1.9.1",
    "winston": "3.3.3"
  },
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:coverage": "jest --coverage",
    "production": "node ./build/server.js",
    "start": "node -r ts-node/register ./src/server.ts",
    "dev": "nodemon -x ts-node --project tsconfig.json src/server.ts",
    "build": "tsc",
    "knex:migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "knex:seed": "knex seed:run",
    "knex:reset": "yarn knex:migrate:rollback && yarn knex:migrate:latest && yarn knex:seed",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "prettier": "prettier  \"**/*.+(js|ts|css|sass|less|graphql|scss|json)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run prettier -- --list-different && npm run lint",
    "local": "ts-node scripts/local"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "swaggerDocs"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts,js,json,yaml,yml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/i18n": "^0.13.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.3",
    "@types/papaparse": "^5.2.4",
    "@types/q": "^1.5.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/underscore": "^1.11.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^27.3.1",
    "lint-staged": "^10.4.2",
    "nodemon": "^1.18.10",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "3.3.0",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "ignore": [
        "node_modules",
        "build"
      ]
    }
  }
}
